{
    "version": 2,
    "builds": [
        {
            "src": "api/rates.py",
            "use": "@vercel/python"
        },
        {
            "src": "api/health.py",
            "use": "@vercel/python"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/rates(.*)",
            "dest": "/api/rates.py"
        },
        {
            "src": "/api/health(.*)",
            "dest": "/api/health.py"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}